{
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "affected": {
    "defaultBase": "master"
  },
  "npmScope": "glibby",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "projects": {
    "glibby-site": {
      "tags": []
    },
    "glibby-site-e2e": {
      "tags": [],
      "implicitDependencies": ["glibby-site"]
    },
    "glibby-grid-animation": {
      "tags": []
    },
    "glibby-core": {
      "tags": []
    }
  }
}
